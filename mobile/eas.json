{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "node": "22.12.0",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EAS_BUILD_GRADLE_MIRROR_ENABLED": "false"
      }
    },
    "preview": {
      "node": "22.12.0",
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EAS_BUILD_GRADLE_MIRROR_ENABLED": "false"
      }
    },
    "production": {
      "node": "22.12.0",
      "env": {
        "EAS_BUILD_GRADLE_MIRROR_ENABLED": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}